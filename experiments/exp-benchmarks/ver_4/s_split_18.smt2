(declare-rel fail ())
(declare-rel inv2 (Int Int Int Int Int))
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(declare-var G Int)
(rule (=> (and (= G C) (= F C) (= B A) (= 1 C) (> A 0) (>= 16 A)) (inv2 G F C B A)))
(rule (let ((a!1 (and (inv2 G F C B A)
                (= D (* G 2))
                (= E (ite (< F B) (* F 2) (mod G 16))))))
  (=> a!1 (inv2 D E C B A))))
(rule (=> (and (inv2 G F C B A) (> G B) (not (= 0 F))) fail))
(query fail)

