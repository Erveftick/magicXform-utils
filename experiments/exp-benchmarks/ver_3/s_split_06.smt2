(declare-rel fail ())
(declare-rel inv2 (Int Int Int Int Int Int))
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(declare-var G Int)
(declare-var H Int)
(declare-var I Int)
(rule (=> (and (= G A) (= D 0) (= I 0) (= A C) (= B (* B C)) (= A C) (= 1 C))
    (inv2 G D I C B A)))
(rule (let ((a!1 (and (inv2 G D I C B A)
                (= E (- G))
                (= H (ite (> G 0) (+ D A) D))
                (= F (ite (> G 0) I (+ I A))))))
  (=> a!1 (inv2 E H F C B A))))
(rule (=> (and (inv2 G D I C B A) (= G A) (= D B) (not (= B I))) fail))
(query fail)

