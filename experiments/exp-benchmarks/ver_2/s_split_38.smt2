(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(rule (=> (and (= F 50000) (= E 0) (> B 0) (>= 5 B) (> A 0) (>= 50000 A))
    (inv2 F E B A)))
(rule (let ((a!1 (and (inv2 F E B A)
                (= C (ite (>= E F) (+ F B) F))
                (= D (ite (>= E F) E (+ E 1))))))
  (=> a!1 (inv2 C D B A))))
(rule (let ((a!1 (and (inv2 F E B A) (> E A) (not (<= (- F E) B)))))
  (=> a!1 fail)))
(query fail)

