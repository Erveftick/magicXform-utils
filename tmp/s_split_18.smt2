(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(rule (=> (and (= F 1) (= E 1) (> A 0) (>= 16 A) (= B A)) (inv2 F E B A)))
(rule (let ((a!1 (and (inv2 F E B A)
                (= C (* F 2))
                (= D (ite (< E B) (* E 2) (mod F 16))))))
  (=> a!1 (inv2 C D B A))))
(rule (=> (and (inv2 F E B A) (> F B) (not (= 0 E))) fail))
(query fail)

