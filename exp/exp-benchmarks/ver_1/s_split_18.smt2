(declare-rel inv2 (Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(rule (=> (and (= E 1) (= D 1) (= 16 A)) (inv2 E D A)))
(rule (let ((a!1 (and (inv2 E D A)
                (= B (* E 2))
                (= C (ite (< D A) (* D 2) (mod E 16))))))
  (=> a!1 (inv2 B C A))))
(rule (=> (and (inv2 E D A) (> E A) (not (= 0 D))) fail))
(query fail)

