(declare-rel inv2 (Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(rule (=> (and (= D 0) (= E 0) (> A 0) (>= 50 A)) (inv2 D E A)))
(rule (let ((a!1 (= B (ite (< (* D 5) E) (+ D 1) (div D 10))))
      (a!2 (= C (ite (< (* D 5) E) E (+ 1 E)))))
  (=> (and (inv2 D E A) a!1 a!2) (inv2 B C A))))
(rule (=> (and (inv2 D E A) (> E A) (not (> E D))) fail))
(query fail)

