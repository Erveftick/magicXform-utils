(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(declare-var G Int)
(rule (=> (and (= E 0) (= B 10) (= G 0) (> A 0) (>= 5 A)) (inv2 E B G A)))
(rule (let ((a!1 (and (inv2 E B G A)
                (= C (mod (+ E 1) 10))
                (= F (mod (- B 1) 10))
                (= D (ite (= E B) 0 (+ G 1))))))
  (=> a!1 (inv2 C F D A))))
(rule (=> (and (inv2 E B G A) (not (<= G A))) fail))
