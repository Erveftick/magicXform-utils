(rule (=> (and (= G 0) (= D C) (= I B) (> A 0) (>= 1000 A) (= C A) (= B (* 2 A)))
    (inv2 G D I C B A)))
(rule (let ((a!1 (and (inv2 G D I C B A)
                (= E (+ 1 G))
                (= H (ite (>= G C) (+ D 1) D))
                (= F (ite (>= D B) (+ I 1) I)))))
  (=> a!1 (inv2 E H F C B A))))
