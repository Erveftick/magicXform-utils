(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int) (x!3 Int) (x!4 Int)) Bool
  (let ((a!1 (not (<= (+ x!0 (* (- 1) x!1) x!2 (* (- 1) x!3)) (- 1))))
        (a!2 (not (>= (+ x!1 (* (- 2) x!2) x!3) 1)))
        (a!3 (not (>= (+ (* 2 x!2) (* (- 1) x!3)) 1)))
        (a!4 (not (<= (+ x!1 (* (- 1) x!2)) (- 1))))
        (a!5 (not (>= (+ x!0 (* (- 1) x!3)) 0)))
        (a!6 (not (<= (+ (* 2 x!2) (* (- 1) x!3)) (- 1))))
        (a!7 (not (<= (+ x!0 (* (- 1) x!3)) 0)))
        (a!8 (not (>= (+ x!0 (* (- 1) x!1) x!2 (* (- 1) x!3)) 1)))
        (a!9 (not (>= (+ x!1 (* (- 1) x!2)) 1))))
    (and (or a!1 a!2)
         (not (<= x!2 0))
         (not (<= x!1 (- 1)))
         a!3
         (or a!4 a!5)
         a!6
         (or a!7 a!8)
         (or a!5 a!9))))

