(rule (=> (and (= G 0) (= D C) (= I 0) (> A 0) (>= 8000 A) (= C A) (= B (* 2 A)))
    (inv2 G D I C B A)))
(rule (let ((a!1 (and (inv2 G D I C B A)
                (= E (+ G 1))
                (= H (ite (>= G C) (+ D 1) (- D 1)))
                (= F (ite (< G C) (+ I 1) (- I 1))))))
