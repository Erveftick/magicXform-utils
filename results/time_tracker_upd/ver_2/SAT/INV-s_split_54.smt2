(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int) (x!3 Int) (x!4 Int) (x!5 Int)) Bool
  (let ((a!1 (not (>= (+ x!0 (* (- 1) x!3)) 0)))
        (a!2 (not (>= (+ x!2 x!0 (* (- 1) x!4)) 1)))
        (a!3 (not (>= (+ x!3 (* (- 1) x!4)) 0)))
        (a!4 (not (<= (+ x!0 (* (- 2) x!3) x!4) (- 1))))
        (a!5 (not (>= (+ x!1 (* (- 1) x!3)) 0)))
        (a!6 (not (<= (+ x!0 (* (- 1) x!4)) (- 1))))
        (a!7 (not (= (mod (+ x!0 x!4) 2) 1)))
        (a!8 (not (<= (+ x!1 (* (- 1) x!0) (* (- 1) x!3) x!4) (- 1))))
        (a!9 (not (<= (+ x!2 x!0 (* (- 1) x!4)) (- 1))))
        (a!10 (not (<= (+ x!2 x!0 (* (- 1) x!4)) (- 3))))
        (a!11 (+ x!2 (* (- 1) x!0) (* 2 x!3) (* (- 1) x!4)))
        (a!12 (not (<= (+ x!1 x!0 (* (- 3) x!3) x!4) (- 1))))
        (a!13 (not (>= (+ x!1 x!0 (* (- 3) x!3) x!4) 1)))
        (a!14 (not (>= (+ x!1 (* (- 1) x!0) (* (- 1) x!3) x!4) 1))))
    (and (or a!1 a!2)
         a!3
         a!4
         (or a!5 a!6 a!7)
         a!8
         (or a!9 a!1)
         (or a!10 (not (<= a!11 (- 1))))
         a!12
         (or a!13 a!14)
         (not (>= a!11 1)))))

