(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int) (x!3 Int)) Bool
  (let ((a!1 (not (= (+ x!0 (* (- 1) x!3)) (- 2))))
        (a!2 (not (= (mod (+ 1 x!0) 2) 0)))
        (a!3 (not (>= (+ x!1 (* (- 1) x!2)) 1)))
        (a!5 (not (<= (+ x!1 (* (- 1) x!2)) (- 1))))
        (a!6 (not (<= (+ x!1 (* (- 1) x!2)) 0)))
        (a!8 (not (= (+ x!0 (* (- 1) x!3)) (- 1))))
        (a!9 (not (>= (+ x!1 (* (- 1) x!2)) 0)))
        (a!11 (not (>= (+ x!1 (* (- 1) x!2)) 2)))
        (a!13 (not (>= (mod (+ 1 x!0) 2) 1))))
  (let ((a!4 (or (not (>= (mod x!0 2) 1)) a!1 a!2 a!3))
        (a!7 (or (not (>= (mod x!0 2) 1)) a!6))
        (a!10 (or (not (= (mod x!0 2) 0)) a!8 a!9))
        (a!12 (or (not (= (mod x!0 2) 0)) a!3)))
    (and a!4 a!5 a!7 a!10 a!11 a!12 (or a!13 a!3) (not (= (mod x!3 2) 1))))))

