(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int) (x!3 Int)) Bool
  (let ((a!1 (not (<= (+ x!2 (* (- 1) x!3)) 0)))
        (a!2 (not (<= (+ x!0 (* (- 1) x!1) (* 2 x!2) (* (- 2) x!3)) 0)))
        (a!3 (not (<= (+ (* 3 x!0) (* (- 2) x!1) (* 9 x!2) (* (- 3) x!3)) 9)))
        (a!4 (not (<= (+ x!0 (* 4 x!2) (* (- 1) x!3)) 3)))
        (a!6 (not (<= (+ x!2 (* (- 1) x!3)) 2)))
        (a!7 (not (<= (+ (* 2 x!2) (* (- 1) x!3)) 0))))
  (let ((a!5 (or (not (= (mod x!2 2) 0)) a!1)))
    (and (not (<= x!2 0))
         (or a!1 a!2)
         (or a!1 a!3)
         (or a!1 a!4)
         a!5
         (or a!6 a!7)))))

