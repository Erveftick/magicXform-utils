(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(rule (=> (and (= E 0) (>= B 0) (= F 0) (= 1000 A)) (inv2 E B F A)))
(rule (let ((a!1 (= D (ite (= B (div E 1000)) (+ F 1) F))))
  (=> (and (inv2 E B F A) (= C (+ 1 E)) a!1) (inv2 C B D A))))
(rule (let ((a!1 (and (inv2 E B F A) (> E (* A (+ 1 B))) (not (= F A)))))
  (=> a!1 fail)))
(query fail)

