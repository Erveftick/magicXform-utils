(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(rule (=> (and (= E 0) (> A 0) (>= 200 A) (= C A) (= B (* 10 A))) (inv2 E C B A)))
(rule (let ((a!1 (= D (ite (< (div E 5) C) (+ E 1) (+ E 5)))))
  (=> (and (inv2 E C B A) a!1) (inv2 D C B A))))
(rule (let ((a!1 (and (inv2 E C B A) (>= E B) (not (= (mod E 5) 0)))))
  (=> a!1 fail)))
(query fail)

