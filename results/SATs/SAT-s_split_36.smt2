(declare-rel inv2 (Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(rule (=> (and (= C A) (= B 0) (= (- 10000) A)) (inv2 C B A)))
(rule (let ((a!1 (and (inv2 C B A)
                (= D (ite (>= B C) (+ C 1) C))
                (= E (ite (>= B C) (- C) (+ B 2))))))
  (=> a!1 (inv2 D E A))))
(rule (let ((a!1 (and (inv2 C B A) (>= C 0) (not (>= C (- B 1))))))
  (=> a!1 fail)))
(query fail)

