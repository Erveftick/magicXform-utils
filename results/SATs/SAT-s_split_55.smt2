(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(declare-var G Int)
(rule (=> (and (= D 0) (= E 0) (distinct G 0) (= 200 A)) (inv2 E D G A)))
(rule (let ((a!1 (and (inv2 D E G A)
                (= B (ite (> G 0) (+ E 1) (- E 2)))
                (= F (+ D 1))
                (= C (ite (= D 100) (- G) G)))))
  (=> a!1 (inv2 F B C A))))
(rule (=> (and (inv2 D G E A) (= D A) (not (<= G 0))) fail))
(query fail)

