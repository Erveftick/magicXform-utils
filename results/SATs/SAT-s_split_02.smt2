(declare-rel fail ())
(declare-rel inv2 (Int Int Int Int Int))
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(declare-var G Int)
(declare-var H Int)
(rule (=> (and (= E A) (= F 0) (= H B) (= 400 B) (= 200 A)) (inv2 F E H B A)))
(rule (let ((a!1 (and (inv2 E F H B A)
                (= C (ite (< E A) (+ F 1) F))
                (= G (+ 1 E))
                (= D (ite (< E A) H (+ H 2))))))
  (=> a!1 (inv2 G C D B A))))
(rule (let ((a!1 (and (inv2 H F E B A) (>= F B) (not (= E (* 2 H))))))
  (=> a!1 fail)))
(query fail)

