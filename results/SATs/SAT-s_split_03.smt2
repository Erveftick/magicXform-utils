(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(rule (=> (and (> C F) (= D 0) (= E 0)) (inv2 D C F E)))
(rule (let ((a!1 (and (inv2 D E A C)
                (= F (+ 1 D))
                (= B (ite (< D A) (+ C 1) (- C 2))))))
  (=> a!1 (inv2 F E A B))))
(rule (=> (and (inv2 D C F E) (> D (+ C F)) (not (<= E 0))) fail))
(query fail)

