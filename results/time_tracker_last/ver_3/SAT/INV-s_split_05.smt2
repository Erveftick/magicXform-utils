(define-fun inv2 ((x!0 Int)
 (x!1 Int)
 (x!2 Int)
 (x!3 Int)
 (x!4 Int)
 (x!5 Int)
 (x!6 Int)) Bool
  (let ((a!1 (not (<= (+ x!2 (* (- 1) x!3)) 0)))
        (a!2 (not (<= (+ x!0 (* 3 x!2) (* (- 1) x!1) (* (- 3) x!3)) 0)))
        (a!3 (not (<= (+ x!0 (* 2 x!4)) 2)))
        (a!4 (not (<= (+ x!2 (* (- 1) x!4)) 0)))
        (a!5 (not (<= (+ x!0 (* (- 1) x!1) x!4) 2))))
