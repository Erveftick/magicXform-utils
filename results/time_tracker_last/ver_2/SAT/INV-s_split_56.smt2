(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int)) Bool
  (let ((a!1 (not (<= (+ x!0 (* (- 1) x!1) x!2) (- 2))))
        (a!2 (not (<= (+ x!1 (* (- 1) x!2)) (- 1))))
        (a!3 (not (>= (+ x!1 (* (- 1) x!2)) 1)))
        (a!4 (not (>= (+ x!0 (* (- 1) x!1) (* (- 1) x!2)) 2)))
        (a!5 (not (>= (+ x!1 (* (- 1) x!2)) 2))))
    (and a!1
         (not (<= x!1 0))
         a!2
         (or a!3 a!4)
         (not (<= x!0 (- 1)))
         (or (not (<= x!0 0)) a!3)
         (or (not (<= x!0 1)) a!5))))

