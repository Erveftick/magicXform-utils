(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(declare-var G Int)
(rule (=> (and (> E 0) (< B 0) (= G 1) (> A 0) (>= 1 A)) (inv2 E B G A)))
(rule (let ((a!1 (and (inv2 E B G A)
                (= C (+ E A))
                (= F (+ B 2))
                (= D (ite (>= B 0) (* G 2) G)))))
  (=> a!1 (inv2 C F D A))))
(rule (=> (and (inv2 E B G A) (>= B E) (not (> G A))) fail))
(query fail)

