(declare-rel fail ())
(declare-rel inv2 (Int Int))
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(rule (=> (and (= D (- 10000)) (= C 0)) (inv2 D C)))
(rule (let ((a!1 (and (inv2 D C)
                (= A (ite (>= C D) (+ D 1) D))
                (= B (ite (>= C D) (- D) (+ C 2))))))
  (=> a!1 (inv2 A B))))
(rule (let ((a!1 (and (inv2 D C) (>= D 0) (not (>= D (- C 1))))))
  (=> a!1 fail)))
(query fail)

