(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int) (x!3 Int) (x!4 Int)) Bool
  (let ((a!1 (not (>= (+ x!1 (* (- 1) x!3) x!4) 0)))
        (a!2 (not (<= (+ x!1 (* (- 1) x!2) x!4) (- 1))))
        (a!4 (not (>= (+ x!1 (* (- 1) x!2)) 1)))
        (a!5 (not (<= (+ x!1 (* (- 1) x!2) (* (- 1) x!4)) (- 1))))
        (a!7 (not (>= (+ x!1 (* (- 1) x!3)) 0)))
        (a!8 (not (<= (+ x!1 (* (- 1) x!2)) (- 1))))
        (a!9 (not (>= (+ x!0 (* (- 1) x!4)) 0)))
        (a!10 (not (<= (+ x!0 (* (- 1) x!4)) 0)))
        (a!11 (not (>= (+ x!1 (* (- 1) x!2) (* (- 1) x!4)) 1)))
        (a!13 (not (>= (+ x!1 (* (- 1) x!2) x!4) 1))))
  (let ((a!3 (or (not (= (+ x!0 x!4) 0)) a!1 a!2))
        (a!6 (or a!5 (not (<= (+ x!0 x!4) 0))))
        (a!12 (or a!10 a!11 (not (= (+ x!0 x!4) 0))))
        (a!14 (or (not (>= x!0 1)) (not (= (+ x!0 x!4) 0)) a!13)))
    (and a!3
         (or (not (= x!0 x!4)) a!4)
         a!6
         (or a!7 (not (= x!0 x!4)) a!8)
         (or a!9 a!8 (not (>= x!0 0)))
         a!12
         a!14))))

