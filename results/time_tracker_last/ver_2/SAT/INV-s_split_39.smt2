(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int)) Bool
  (let ((a!1 (not (>= (+ x!1 (* (- 1) x!2)) 1)))
        (a!2 (not (>= (+ x!0 (* (- 1) x!1)) (- 1))))
        (a!3 (not (<= (+ (* 5 x!0) (* (- 1) x!1)) (- 1))))
        (a!4 (<= (+ x!1 (* (- 1) (div x!0 10))) (- 1)))
        (a!5 (not (>= (+ x!0 (* (- 1) x!1)) 0)))
        (a!6 (not (>= (+ x!1 (* (- 1) x!2)) 0)))
        (a!7 (<= (+ x!1 (* (- 1) (div x!0 10))) 0)))
    (and (or a!1 a!2 a!3)
         (not a!4)
         (or a!1 a!5)
         (or a!6 (not a!7))
         (not (<= x!0 (- 1))))))

