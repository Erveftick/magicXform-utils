(rule (=> (and (= F 0) (= C 1000) (= H 2000) (> B 0) (>= 1000 B) (> A 0) (>= 2000 A))
    (inv2 F C H B A)))
(rule (let ((a!1 (and (inv2 F C H B A)
                (= D (+ 1 F))
                (= G (ite (>= F B) (+ C 1) C))
                (= E (ite (>= C A) (+ H 1) H)))))
  (=> a!1 (inv2 D G E B A))))
(rule (=> (and (inv2 F C H B A) (>= C A) (not (= F H))) fail))
