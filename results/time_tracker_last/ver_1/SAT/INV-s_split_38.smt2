(define-fun inv2 ((x!0 Int) (x!1 Int) (x!2 Int) (x!3 Int)) Bool
  (let ((a!1 (not (>= (+ x!0 (* (- 1) x!1) (* (- 1) x!2)) 3)))
        (a!2 (not (>= (+ x!0 (* (- 1) x!2) (* (- 1) x!3)) 2)))
        (a!3 (not (>= (+ x!1 (* (- 1) x!3)) 1)))
        (a!4 (not (>= (+ x!0 (* (- 1) x!1) (* (- 1) x!2)) 1)))
        (a!5 (not (>= (+ x!0 (* (- 1) x!1) (* (- 1) x!2)) 2)))
        (a!6 (not (>= (+ x!0 (* (- 1) x!1)) 1)))
        (a!7 (not (>= (+ x!1 (* (- 1) x!3)) (- 1))))
        (a!8 (not (>= (+ x!0 (* (- 1) x!3)) 1))))
    (and (or a!1 a!2) (or a!3 a!4) (or a!5 a!6 a!7 a!8 a!2))))

