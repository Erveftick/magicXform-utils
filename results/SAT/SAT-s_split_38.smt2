(declare-rel inv2 (Int Int Int Int))
(declare-rel fail ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(rule (=> (and (= D A) (= C 0) (= 5 B) (= 50000 A)) (inv2 D C B A)))
(rule (let ((a!1 (and (inv2 D C B A)
                (= E (ite (>= C D) (+ D B) D))
                (= F (ite (>= C D) C (+ C 1))))))
  (=> a!1 (inv2 E F B A))))
(rule (let ((a!1 (and (inv2 D C B A) (> C A) (not (<= (- D C) B)))))
  (=> a!1 fail)))
(query fail)

